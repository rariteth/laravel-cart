{
  "name": "rariteth/laravel-cart",
  "description": "Laravel Shopping Cart",
  "keywords": [
    "laravel",
    "shoppingcart",
    "cart"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Raysky",
      "email": "rariteth@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "5.*|6.*|7.*|8.*",
    "beberlei/assert": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "mockery/mockery": "~1",
    "orchestra/testbench": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "Rariteth\\LaravelCart\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rariteth\\LaravelCart\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
        "sort-packages": true
    },
  "extra": {
    "laravel": {
      "providers": [
        "Rariteth\\LaravelCart\\CartServiceProvider"
      ]
    }
  }
}
